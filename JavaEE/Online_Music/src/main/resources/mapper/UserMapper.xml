<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.online_music.dao.UserMapper">


    <insert id="addUser" parameterType="com.example.online_music.model.User">
        insert into user (username,password,age,gender,email) values
        (#{userName},#{password},#{age},#{gender},#{email})
    </insert>
    <select id="findUserByName" resultType="com.example.online_music.model.User">
        select * from user where username = #{username}
    </select>
</mapper>